{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to running Delve debug process",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "port": 4444,
      "host": "127.0.0.1",
      "substitutePath": [
        {
          "from": "${workspaceFolder}",
          "to": "/usr/src/app"
        }
      ]
    }
  ]
}
